<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.restaurant">
	<insert id="">
		insert into restaurant(resNo,memNo,foodTypeNo,resTypeNo,resName,
			resIntroduce,resWorkDay,resWorkHour,resThumbnail,resGrade,
			resMaxPerson,resContact,resZipcode,resAddress,resAddressDetail)
			values(#{resNo},#{memNo},#{foodTypeNo},#{resTypeNo},#{resName},
			#{resIntroduce},#{resWorkDay},#{resWorkHour},#{resThumbnail},#{resGrade},
			#{resMaxPerson},#{resContact},#{resZipcode},#{resAddress},#{resAddressDetail})
	</insert>
	
	
</mapper>